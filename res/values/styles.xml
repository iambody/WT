<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppBaseTheme" parent="android:Theme.Light"></style>

    <style name="AppTheme" parent="AppBaseTheme">
        <item name="android:windowNoTitle">true</item>

    </style>
    <!-- 弹出框选择确定取消的style——————@author大兔兔 -->
    <style name="mystyle" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/shape_bg1</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:inAnimation">@anim/dialog_in</item>
        <item name="android:outAnimation">@anim/dialog_out</item>
    </style>

    <style name="main_menu_animstyless">
        <item name="android:windowEnterAnimation">@anim/dialog_in</item>
        <item name="android:windowExitAnimation">@anim/dialog_out</item>
    </style>

    <!-- load时候的动画@author大兔兔 -->
    <style name="loading" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="loadingStyle">
        <item name="android:indeterminateDrawable">@anim/load_anima</item>
    </style>
    <!-- 加载loading -->
    <style name="myloading">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:windowFullscreen">false</item>
        <!-- <item name="android:windowNoTitle">false</item> -->
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:adjustViewBounds">true</item>

        <!-- 模糊 -->
        <item name="android:windowIsFloating">false</item>
        <!-- 类似windows的弹出Dialog -->
        <item name="android:windowContentOverlay">@null</item>
        <!-- 不要头部阴影 -->
        <item name="android:windowBackground">@color/transparent</item>
        <!-- 背景颜色设置透明 -->
        <item name="android:background">@android:color/transparent</item>
    </style>
    <!-- 加载loading -->
    <style name="myloading3">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:windowFullscreen">false</item>
        <!-- <item name="android:windowNoTitle">false</item> -->
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:adjustViewBounds">true</item>

        <!-- 模糊 -->

        <item name="android:windowIsFloating">false</item>
        <!-- 类似windows的弹出Dialog -->
        <item name="android:windowContentOverlay">@null</item>
        <!-- 不要头部阴影 -->
        <item name="android:windowBackground">@color/transparent</item>
        <!-- 背景颜色设置透明 -->
        <item name="android:background">@color/transparent3</item>
    </style>
    <!-- 上边imagview下边textview的布局 -->
    <style name="iv_text">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:padding">6dp</item>
    </style>
    <!-- 首页图片 -->
    <style name="home_good">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:scaleType">centerCrop</item>
    </style>
    <!-- share -->
    <style name="share_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:padding">10dp</item>
    </style>
    <!-- 极光推送 -->
    <style name="push_style">
        <item name="android:hint">@string/style_hint</item>
        <item name="android:inputType">number</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="push_alias">
        <item name="android:hint">@string/alias_hint</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="push_tag">
        <item name="android:hint">@string/tag_hint</item>
        <item name="android:textSize">14sp</item>
    </style>
    <!-- 极光推送************** -->

    <style name="btnstyle" parent="@android:style/Widget.Button">
        <item name="android:textColor">@drawable/select_btn_text_bg</item>
        <item name="android:background">@drawable/select_fen_to_gray_no_circular</item>
    </style>
    <style name="clickstyle" >
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:background">@drawable/select_white_to_gray</item>
    </style>
